{%
  set data_attributes = {
    'data-categories': categories|join(',')
  }
%}
<div {{ attributes.addClass(data_attributes) }}>
  <div class="session-schedule--type">{{ type }}</div>
  {% if pre_tags %}
  <ul class="session-schedule--pre-tags ul--pills--inline ul--pills--small">
    {% for tag in pre_tags %}
      {% if tag|render|striptags is not empty %}
        {{ tag }}
      {% endif %}
    {% endfor %}
  </ul>
  {% endif %}
  {{ title }}
  <div class="speakers">{{ speakers }}</div>
  <ul class="session-schedule--tags ul--pills--inline ul--pills--small">
    {{ tags }}
  </ul>
</div>
